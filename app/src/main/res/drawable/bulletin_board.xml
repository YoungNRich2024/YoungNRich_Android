<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke
                android:color="@color/dark_blue"
                android:width="8dp" />
        </shape>
    </item>
    <item
        android:top="16dp"
        android:bottom="16dp"
        android:start="16dp"
        android:end="16dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke
                android:color="@color/dark_blue"
                android:width="6dp" />
        </shape>
    </item>
</layer-list>
